
<div class="content">
  <h1>Dashboard#index</h1>

  <p>Find me in app/views/dashboard/index.html.erb</p>
  <% if user_signed_in? %>
    <p>Name: <%= current_user.name %></p>
    <p>Email: <%= current_user.email %></p>
    <p><%= image_tag current_user.avatar if current_user.avatar.present?  %></p>
  <% end %>

</div>
