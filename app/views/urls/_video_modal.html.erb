<div class="post_modal modal fade" id="video-modal" tabindex="-1" role="dialog" aria-labelledby="embed-modalTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Embed (Video, Tweets, HTML)
        </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
         <div id="video_tag">
          <%= simple_form_for setup_video_tag(VideoTag.new), url: create_path(url: params[:url]),remote: true, method: :post do |f| %>
              <%= f.hidden_field :user_id, value: current_user.id %>
              <%= f.fields_for :video_datum do |i| %>
                <%#= i.input :category %>
                <div class="form-group">
                      <label>Category</label>
                      <div class="row">
                        <div class="col-md-4">
                          <div class="radio">
                            <label>
                              <%= i.radio_button :category, id: 'category', name: 'category', type: 'radio', value: 'SearchEng.in' %>
                              <span>SearchEng.in</span>
                            </label>
                          </div>
                        </div>
                        <div class="col-md-4">
                          <div class="radio">
                            <label>
                              <%= i.radio_button :category, id: 'category', name: 'category', type: 'radio', value: 'SearchEng.in' %>
                                <span>SearchEng.in</span>
                            </label>
                          </div>
                        </div>
                        <div class="col-md-4">
                          <div class="radio">
                            <label>
                              <%= i.radio_button :category, id: 'category', name: 'category', type: 'radio', value: 'Twitter' %>
                                <span>Twitter</span>
                              </label>
                          </div>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-md-4">
                          <div class="radio">
                            <label>
                              <%= i.radio_button :category, id: 'category', name: 'category', type: 'radio', value: 'Instagram' %>
                              <span>Instagram</span>
                            </label>
                          </div>
                        </div>
                        <div class="col-md-4">
                          <div class="radio">
                            <label>
                              <%= i.radio_button :category, id: 'category', name: 'category', type: 'radio', value: 'Facebook' %>
                                <span>Facebook</span>
                            </label>
                          </div>
                        </div>
                        <div class="col-md-4">
                          <div class="radio">
                            <label>
                              <%= i.radio_button :category, id: 'category', name: 'category', type: 'radio', value: 'Other' %>
                                <span>Other</span>
                              </label>
                          </div>
                        </div>
                     </div>
                     <div class="radio">
                      <label>
                         <%= i.radio_button :category, id: 'category', name: 'category', type: 'radio', value: 'Other' %>
                          <span>Other</span>
                        </label>
                    </div>
                    <div class="form-group other">
                        <input class="form-control" id="category" name="category" type="text">
                    </div>
                <%= i.input :html_code %>
                <%= i.input :video_url %>
                <%= i.input :description %>
              </div>
              <% end %>
              <div class="modal-footer" style="float: left;border:none;padding: 0px;">
                <%= f.submit "Submit", class: "btn default_btn" %>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
