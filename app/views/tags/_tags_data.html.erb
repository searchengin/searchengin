<div class="reslt_body wdth100">
  <div class="reslt_row wdth100">
    <span class="arrow left"></span>
    <div class="reslt_col wdth100">
      <div class="wdth100">
        <% if params.has_key?(:where_tag) %>
          <div>
            <%= @data.where_datum.location %>
            <%= @data.where_datum.description %>
          </div>
        <% elsif params.has_key?(:what_tag) %>
          <div>
            <%= @data.what_datum.item_name %>
            <%= @data.what_datum.category %>
            <%= @data.what_datum.website_url %>
            <%= @data.what_datum.description %>
          </div>
        <% elsif params.has_key?(:why_tag) %>
          <div>
            <%= @data.why_datum.subject %>
            <%= @data.why_datum.subcategory %>
            <%= @data.why_datum.title %>
            <%= @data.why_datum.description %>
          </div>
        <% elsif params.has_key?(:when_tag) %>
          <div>
            <%= @data.when_datum.date %>
            <%= @data.when_datum.time %>
            <%= @data.when_datum.description %>
          </div>
        <% elsif params.has_key?(:who_tag) %>
          <div>
            <%= @data.who_datum.name %>
            <%= @data.who_datum.account %>
            <%= @data.who_datum.website_url %>
            <%= @data.who_datum.description %>
          </div>
        <% elsif params.has_key?(:video_tag) %>
          <div>
            <%= @data.video_datum.video_url %>
            <%= @data.video_datum.category %>
            <%= @data.video_datum.html_code %>
            <%= @data.video_datum.description %>
          </div>
         <% elsif params.has_key?(:photo_tag) %>
          <div>
            <%= @data.photo_datum.photo_url %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <ul class="post_likes">
    <a class="url_data" data-remote="true" href="/dashboard/get_url_object/28">
     <i class="fas fa-code"></i>
    </a>
    <li>
      <a data-remote="true" rel="nofollow" data-method="post" href="/dashboard/like?url=28">
      <i class="fas fa-thumbs-up text-success" id="like_28">0</i>
      </a>
    </li>
    <li>
      <a data-remote="true" rel="nofollow" data-method="post" href="/dashboard/dislike?url=28">
      <i class="fas fa-thumbs-down" id="dislike_28">0</i>
      </a>
    </li>
  </ul>
</div>
